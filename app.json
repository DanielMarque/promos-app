{
  "expo": {
    "name": "promos-app",
    "slug": "promos-app",
    "scheme": "promos-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "intentFilters": [
        {
          "autoVerify": true,
          "action": "VIEW",
          "data": {
            "scheme": "https",
            "host": "*usat5wglha.execute-api.us-east-1.amazonaws.com*",
            "pathPrefix": "/promos/oauth*"
          },
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF"
      },
      "package": "android.package"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
